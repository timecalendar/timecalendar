{
  "name": "@timecalendar/timecalendar",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "postinstall": "husky install",
    "app:tsnode": "ts-node -r tsconfig-paths/register -r dotenv/config",
    "typeorm": "yarn app:tsnode ./node_modules/typeorm/cli -f ./src/ormconfig.ts",
    "db:seed": "yarn app:tsnode ./src/scripts/seed-database.ts",
    "db:init": "yarn app:tsnode ./src/scripts/seed-database.ts --drop"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ],
    "*.dart": [
      "flutter format",
      "./bin/flutter-analyze.sh"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/microservices": "^8.0.6",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^8.0.2",
    "@types/ejs": "^3.1.0",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.178",
    "@types/nodemailer": "^6.4.4",
    "axios": "^0.25.0",
    "bull": "^3.29.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "commander": "^9.0.0",
    "compression": "^1.7.4",
    "cron": "^1.8.2",
    "date-fns": "^2.23.0",
    "dayjs": "^1.11.1",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "firebase-admin": "^10.0.0",
    "fishery": "^2.1.0",
    "glob": "^7.2.0",
    "glob-promise": "^4.2.2",
    "helmet": "^4.6.0",
    "ioredis-mock": "^7.2.0",
    "lodash": "^4.17.21",
    "mockdate": "^3.0.5",
    "mysql2": "^2.3.3",
    "nanoid": "^3.2.0",
    "node-ical": "^0.14.1",
    "nodemailer": "^6.6.3",
    "qs": "^6.10.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "slug": "^5.1.0",
    "ssl-root-cas": "^1.3.1",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bull": "^3.15.4",
    "@types/compression": "^1.7.2",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/slug": "^5.0.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "jest-junit": "^13.0.0",
    "lint-staged": ">=10",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typeorm-fixtures-cli": "^1.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.(spec|test)\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "globalSetup": "./global-setup.ts",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFilesAfterEnv": [
      "./setup-tests.ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "runner": "./test-utils/test-runner.ts"
  }
}
