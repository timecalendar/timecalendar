    "@typescript-eslint/parser": "^6.1.0",
    "axios": "^1.4.0",
    "bullmq": "^5.34.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "commander": "^11.0.0",
    "compression": "^1.7.4",
    "crisp-api": "^8.0.1",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^5.0.0",
    "express-basic-auth": "^1.2.1",
    "firebase-admin": "^11.10.1",
    "fishery": "^2.1.0",
    "glob": "^7.1.6",
    "glob-promise": "^4.2.2",
    "helmet": "^7.0.0",
    "https-proxy-agent": "^7.0.2",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.6",
    "node-ical": "^0.14.1",
    "nodemailer": "^6.9.4",
    "p-limit": "^3.1.0",
    "pg": "^8.11.1",
    "prettier": "^3.0.3",
    "pretty-hrtime": "^1.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "slug": "^8.2.3",
    "slugify": "^1.6.6",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.17",
    "typeorm-fixtures-cli": "^3.1.0",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "wtfnode": "^0.9.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.(spec|test)\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "globalSetup": "./global-setup.ts",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFilesAfterEnv": [
      "./setup-tests.ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "runner": "./test-utils/test-runner.ts",
    "clearMocks": true
  },
  "devDependencies": {
    "msw": "^2.7.0"
  }
