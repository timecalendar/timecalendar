{
  "name": "@timecalendar/timecalendar",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "app:tsnode": "ts-node -r tsconfig-paths/register -r dotenv/config",
    "typeorm": "npm run app:tsnode ./node_modules/typeorm/cli -- -d src/data-source.ts",
    "db:seed": "npm run app:tsnode ./src/scripts/seed-database.ts",
    "db:init": "npm run app:tsnode ./src/scripts/seed-database.ts -- --drop",
    "db:migrate": "npm run typeorm migration:run"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "@bull-board/express": "^5.6.1",
    "@nestjs/bullmq": "^10.2.3",
    "@nestjs/cli": "^10.1.10",
    "@nestjs/common": "^10.1.0",
    "@nestjs/core": "^10.1.0",
    "@nestjs/microservices": "^10.1.0",
    "@nestjs/platform-express": "^10.1.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/testing": "^10.1.0",
    "@nestjs/typeorm": "^10.0.0",
    "@opentelemetry/auto-instrumentations-node": "^0.55.2",
    "@opentelemetry/exporter-trace-otlp-http": "^0.57.1",
    "@opentelemetry/sdk-node": "^0.57.1",
    "@types/compression": "^1.7.2",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/pretty-hrtime": "^1.0.1",
    "@types/slug": "^5.0.2",
