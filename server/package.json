{
  "name": "@timecalendar/timecalendar",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "app:tsnode": "ts-node -r tsconfig-paths/register -r dotenv/config",
    "typeorm": "npm run app:tsnode ./node_modules/typeorm/cli -- -d src/data-source.ts",
    "db:seed": "npm run app:tsnode ./src/scripts/seed-database.ts",
    "db:init": "npm run app:tsnode ./src/scripts/seed-database.ts -- --drop",
    "db:migrate": "npm run typeorm migration:run"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "@bull-board/express": "^4.2.2",
    "@nestjs/bull": "^0.6.0",
    "@nestjs/common": "^9.0.4",
    "@nestjs/core": "^9.0.5",
    "@nestjs/microservices": "^9.0.4",
    "@nestjs/platform-express": "^9.0.4",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/typeorm": "^9.0.0",
    "@types/ejs": "^3.1.0",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.178",
    "@types/nodemailer": "^6.4.4",
    "@types/pretty-hrtime": "^1.0.1",
    "@types/wtfnode": "^0.7.0",
    "axios": "^0.27.2",
    "bull": "^4.8.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "commander": "^9.0.0",
    "compression": "^1.7.4",
    "cron": "^1.8.2",
    "date-fns": "^2.23.0",
    "dayjs": "^1.11.1",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.7",
    "express-basic-auth": "^1.2.1",
    "firebase-admin": "^10.0.0",
    "fishery": "^2.1.0",
    "glob": "^7.2.0",
    "glob-promise": "^4.2.2",
    "helmet": "^4.6.0",
    "ioredis-mock": "^7.2.0",
    "lodash": "^4.17.21",
    "mockdate": "^3.0.5",
    "nanoid": "^3.3.4",
    "node-ical": "^0.14.1",
    "nodemailer": "^6.6.3",
    "p-limit": "^3.1.0",
    "pg": "^8.7.3",
    "pretty-hrtime": "^1.0.3",
    "qs": "^6.10.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "slug": "^5.1.0",
    "ssl-root-cas": "^1.3.1",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.3.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1",
    "wtfnode": "^0.9.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.4",
    "@types/bull": "^3.15.9",
    "@types/compression": "^1.7.2",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/slug": "^5.0.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "jest-junit": "^13.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typeorm-fixtures-cli": "^3.0.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.(spec|test)\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "globalSetup": "./global-setup.ts",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFilesAfterEnv": [
      "./setup-tests.ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "runner": "./test-utils/test-runner.ts"
  }
}
